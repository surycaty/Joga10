<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.surycaty.joga10">

    <application
            android:allowBackup="true"
            android:icon="@drawable/logo_app"
            android:label="@string/app_name"
            android:roundIcon="@drawable/logo_app_around"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.TimesFormadosActivity"/>
        <activity android:name=".activity.SelecionarJogadoresActivity"/>
        <activity android:name=".activity.CadastroJogadorActivity"/>
        <activity android:name=".activity.GerenciarAtletaActivity">
        </activity>
    </application>

</manifest>